  100 Sub ptrcal local linect,strt,stp,begin,abrt1,done,start  110   !  120   !  ***********************  130   !  * PRINTER CALIBRATION *  140   !  ***********************  150   !  160   ! Initialization  170   !  180     On abort then gosub abrt1  190     Print "PRINTER CALIBRATION"  200     Integer linect  210     Open #3:"FRTP(RATE=0,VIEW=0):"  220     Open #2:"PRIN:"  230     Linect=0  240     Set proceed 1  250     Print #2:"88888888888888888888"  260     Begin=ask("time")  270     Stp=begin  280   !  290   ! Perform Calibration  300   !  310 Start:   !  320     Strt=stp  330     Print #2:"88888888888888888888"  340     Stp=ask("time")  350     Linect=linect+1  360     Print #3 using "6.2G,' SECONDS'":stp-strt  370     Goto start  380   !  390   ! End of test  400   !  410 Done:   !  420     Set proceed 0  430     Print #3 using "6.2g,' LINES/SEC '":(linect/(stp-begin))  440     Print #2:"LINES/SEC ";(linect/(stp-begin))  450     Wait 3  460     Close 2,3  470     Print "PRINTER CALIBRATION EXIT"  480     Return  490 Abrt1:   ! <ABORT> handler  500     Branch done  510     End