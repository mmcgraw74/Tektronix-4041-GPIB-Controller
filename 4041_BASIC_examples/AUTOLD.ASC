100Start:init all110REM Open#0:"COMM:"120REM Set cons"COMM:"130REM Set driv"COMMO(BAU=9600):"140PRIN"Console FRTP:(1) or comm:(0)";150Rbyt#1:b$160REM Disa keys170Let drv$=" "180Prin#1:190Prin#1:200If val(b$)=1 then con$="FRTP:"else gosu get_em210If drv$<>" "then set driv drv$220Set cons con$230If seg$(ask$("time"),1,9)<>"01-jan-81"then goto movon else inpu prom"please enter date & time DD-MMM-YY HH:MM:SS ":a$240Set time a$250Movon:prin"THE DATE & TIME ARE CURRENTLY ";ask$("time")260Prin"THE CURRENT CONSOLE PARAMENTERS ARE:"270Prin ask$("console")280Inpu prom"Do you wish to run a program now? ":a$290If a$="y"or a$="yes"then dir  else goto makeit300Inpu prom"enter the name of the program you wish ":a$310Load a$320Get_em:!GET THE PARAMETERS
330Prin#1:"1=STO 2=RAS"340Rbyt#1:b$350Prin#1:360Prin#1:370If val(b$)=1 then file$="sconfig"else file$="rconfig"380Readit:let file$="tape:"&file$&"(open=old)"390Open#2:file$400Inpu#2:drv$410Inpu#2:con$420Clos 2430Retu440Makeit:!CREATE CONFIG ON NEW TAPE450Inpu prom"create a config file? ":a$460If a$="y"or a$="yes"then prin"creating file"else goto finish470Prin"reading values"480Let file$="sconfig"490Gosu readit500Let drv1$=drv$510Let con1$=con$520Let file$="rconfig"530Gosu readit540Dism 550Inpu prom"insert new tape & press return":a$560Let file$="tape:rconfig(open=new,clip=yes)"570Open#2:file$580Prin#2:drv$590Prin#2:con$600Clos 2610Let file$="tape:sconfig(open=new,clip=yes)"620Open#2:file$630Prin#2:drv1$640Prin#2:con1$650Clos 2660Prin"files created"670Goto movon680Finish:end